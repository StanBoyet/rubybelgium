(function() {
  var display_meetup_events, event_icon, filter_attribute, format_date, get_meetup_events, ready;

  ready = function() {
    return get_meetup_events();
  };

  get_meetup_events = function() {
    var BASE_URL, EVENTS_URL, GROUP, PARAMS;
    GROUP = 'ruby_belgium';
    BASE_URL = 'https://api.meetup.com';
    EVENTS_URL = BASE_URL + "/" + GROUP + "/events";
    PARAMS = {
      fields: 'group_photo'
    };
    return $.ajax(EVENTS_URL, {
      dataType: 'jsonp',
      data: PARAMS,
      success: function(response) {
        return display_meetup_events(response.data);
      }
    });
  };

  display_meetup_events = function(events) {
    var template;
    if (events.length === 0) {
      return;
    }
    template = $.templates("#event-template");
    $('#events-list li.more').remove();
    return $(events).each(function(index, event) {
      var html;
      html = template.render({
        "class": (index % 2 !== 0 ? 'timeline-inverted' : void 0),
        link: filter_attribute(event.link),
        image: event_icon(event.name) || filter_attribute(event.group.photo.photo_link),
        date: format_date(event.time),
        title: event.name,
        description: event.venue.name
      });
      return $('#events-list').append(html);
    });
  };

  event_icon = function(event_name) {
    if (event_name.match(/BRUG/i)) {
      return image_path('brug-icon.png');
    } else if (event_name.match(/burger/i)) {
      return image_path('rubyburgers-icon.png');
    } else {
      return void 0;
    }
  };

  filter_attribute = function(attribute) {
    return attribute.replace(/[\t\n\f >"']/g, '');
  };

  format_date = function(timestamp) {
    var date;
    date = new Date(parseInt(timestamp));
    return (date.getDate()) + "/" + (date.getMonth() + 1) + "/" + (date.getFullYear());
  };

  $(document).ready(ready);

}).call(this);
